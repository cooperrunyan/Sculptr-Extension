{
  "name": "sculptr",
  "publisher": "BurtMacklyn",
  "displayName": "Sculptr",
  "description": "Creates a project structure",
  "icon": "img/icon.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "url": "https://github.com/cooperrunyan/Sculptr-Extension.git"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sculptr.build",
        "title": "Sculptr: Create Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.4",
    "@types/vscode": "^1.60.0",
    "@vscode/test-electron": "^1.6.2",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  }
}
